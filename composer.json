{
    "name": "krokedil/dintero-checkout-for-woocommerce",
    "description": "Dintero Checkout For WooCommerce",
    "version": "1.12.0",
    "authors": [
        {
            "name": "krokedil",
            "email": "info@krokedil.se"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "wp-coding-standards/wpcs": "^2.3",
        "wpify/scoper": "3.1.3"
    },
    "config": {
        "platform": {
            "php": "7.4"
        },
        "allow-plugins": {
            "wpify/scoper": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "wpify-scoper": {
            "prefix": "KrokedilDinteroCheckoutDeps",
            "folder": "dependencies",
            "autorun": true,
            "globals": [
                "wordpress",
                "woocommerce"
            ]
        }
    },
    "minimum-stability": "dev"
}
